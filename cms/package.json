{
  "name": "@busrom/cms",
  "version": "1.0.0",
  "private": true,
  "description": "Keystone 6 CMS for Busrom - Headless backend with PostgreSQL + AWS S3",
  "scripts": {
    "dev": "node scripts/watch-and-patch-auth.js & keystone dev",
    "patch-auth": "node scripts/patch-auth-pages.js",
    "build": "keystone build && npm run patch-auth",
    "postbuild": "npm run patch-auth",
    "start": "keystone start",
    "migrate": "keystone prisma migrate dev",
    "postinstall": "prisma generate || echo 'Skipping prisma generate (will be generated by Keystone)'",
    "seed:contactform": "tsx ../scripts/seed-contactform.ts",
    "seed:formconfig": "tsx ../scripts/seed-formconfig.ts",
    "seed:navigation": "tsx scripts/reseed-navigation.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.705.0",
    "@aws-sdk/s3-request-presigner": "^3.705.0",
    "@keystone-6/auth": "^8.0.0",
    "@keystone-6/core": "^6.3.1",
    "@keystone-6/fields-document": "^9.0.1",
    "@prisma/client": "^5.22.0",
    "@types/multer": "^2.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/speakeasy": "^2.0.10",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.15",
    "qrcode": "^1.5.4",
    "react-hook-form": "^7.66.0",
    "sharp": "^0.33.5",
    "speakeasy": "^2.0.0",
    "uuid": "^11.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/nodemailer": "^6.4.16",
    "pg": "^8.16.3",
    "prisma": "^5.22.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=24.11.0"
  }
}
